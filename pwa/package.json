{
  "name": "pwa",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "gcp-build": "",
    "dev": "node ./server.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "generate-api": "oazapfts ../src/backend/web/static/swagger/api_v3.json --argumentStyle=object > app/api/v3.ts",
    "generate-pwa-assets": "pwa-assets-generator",
    "bundle-visualizer": "npx vite-bundle-visualizer",
    "test": "vitest ./app --coverage"
  },
